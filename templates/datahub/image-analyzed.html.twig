{% extends 'base.html.twig' %}
{% block title %}

<title>Test</title>
{% endblock %}
{% block body %}

            <!-- ============================================================== -->
            <!-- Start Page Content here -->
            <!-- ============================================================== -->
            <div class="content-page">
                <div class="content">
                    <!-- Start Content-->
                    <div class="container-fluid">
                        <!-- start page title -->
                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box">
                                    <div class="page-title-right">
                                       
                                    </div>
                                    <h4 class="page-title">Image Analyze</h4>
                                </div>
                            </div>
                        </div>     
                        <!-- end page title --> 
                        <div class="row">
                            <div class="col-8">
                                <div class="card">
                                    <div class="card-body">
                                        <div>
                                            <img src="{{img}}" style="max-width:100%;"/>
                                        </div>
                                        
                                    </div> <!-- end card body-->
                                </div> <!-- end card -->
                            </div>
                            <div class="col-4">
                                <div class="card">
                                    <div class="card-body">
                                        <div>
                                            <button type="button" data-toggle="modal" id="faces-details-data" class="btn btn-primary" style="margin-top:5px;margin-bottom:15px;border-radius:0px;">Face</button>
                                            <button type="button" data-toggle="modal" id="text-details-data" class="btn btn-primary" style="margin-top:5px;margin-bottom:15px;border-radius:0px;">Text</button>

                                        </div>
                                        <div id="text-details" style="display:none;">
                                            {% if webs is null %}
                                            <div>
                                                <h5 style="display:inline-block">No Data</h5>
                                            </div>
                                            {% endif %}
                                            {% if webs is not null %}
                                                {% for web in webs %}
                                                <div>
                                                    <h5 style="display:inline-block">Description : </h5>
                                                    <i style="display:inline-block">{{ web.description }}</i>
                                                </div>
                                                {% endfor %}
                                            {% endif %}
                                        </div>
                                        <div id="faces-details">
                                            {% if faces is null %}
                                                <div>
                                                    <h5 style="display:inline-block">No Data Faces</h5>
                                                </div>
                                            {% endif %}
                                            {% if faces is not null %}
                                                {% for face in faces %}

                                                <div>
                                                    <h5 style="display:inline-block">Joy : </h5>
                                                    <i style="display:inline-block">{{ face.joy }}</i>
                                                </div>
                                                <div>
                                                    <h5 style="display:inline-block">Sorrow : </h5>
                                                    <i style="display:inline-block">{{ face.sorrow }}</i>
                                                </div>
                                                <div>
                                                    <h5 style="display:inline-block">Anger : </h5>
                                                    <i style="display:inline-block">{{ face.anger }}</i>
                                                </div>
                                                <div>
                                                    <h5 style="display:inline-block">Surprise : </h5>
                                                    <i style="display:inline-block">{{ face.surprised }}</i>
                                                </div>
                                                <div>
                                                    <h5 style="display:inline-block">Blurred : </h5>
                                                    <i style="display:inline-block">{{ face.blurred }}</i>
                                                </div>
                                                <div>
                                                    <h5 style="display:inline-block">Headwear : </h5>
                                                    <i style="display:inline-block">{{ face.headwear }}</i>
                                                </div>
                                                {% endfor %}
                                            {% endif %}
                                        </div>
                                        
                                    </div> <!-- end card body-->
                                </div> <!-- end card -->
                            </div><!-- end col-->
                        </div>
                        <!-- end row-->
                    </div> <!-- container -->
                </div> <!-- content -->
            </div>
            <!-- ============================================================== -->
            <!-- End Page content -->
            <!-- ============================================================== -->
        </div>
        <!-- END wrapper -->
