    <script>
    $('document').ready(function(){
        $("#faces-details-data").click(function(){
            console.log("faces")
            $("#faces-details").css("display","block");
            $("#text-details").css("display","none");
        })

        $("#text-details-data").click(function(){
            console.log("text")
            $("#faces-details").css("display","none");
            $("#text-details").css("display","block");
        })

    });

</script>
   
{% endblock %}

        
    
</body>
{% endblock %}
